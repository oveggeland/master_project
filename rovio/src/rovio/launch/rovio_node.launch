<?xml version="1.0" encoding="UTF-8"?> 
<launch>
  <node pkg="rovio" type="rovio_node" name="rovio" output="screen">
  <param name="filter_config" value="$(find rovio)/cfg/b1_rovio.info"/>
  <param name="camera0_config" value="$(find rovio)/cfg/cam0.yaml"/>
  <param name="camera1_config" value="$(find rovio)/cfg/cam1.yaml"/>
  <remap from="imu0" to="/imu_acc_ar" />
  <remap from="cam0/image_raw" to="/cam0/cam0" />
  <remap from="cam0/image_raw" to="/cam0/cam0" />
  </node>
</launch>
